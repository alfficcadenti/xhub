  "main": "./build/index.js",
  "scripts": {
    "automation": "concurrently 'npm start' 'npm run test:browser:local'",
    "build": "bash ./generate-build-info.sh && npm run build:server && npm run build:client && npm run tool:checkAssetSize",
    "build:analyzer": "BABEL_ENV=client NODE_ENV=production webpack --progress --display-modules --config tools/webpack/analyzer.js",
    "build:client": "DEBUG=catalyst* BABEL_ENV=client NODE_ENV=production webpack --config ./tools/webpack/production.js",
    "build:server": "rm -rf ./build/server && rm -rf ./build/universal && BABEL_ENV=server babel src --copy-files --source-maps --ignore='**/*.test.js,**/*.spec.js' --out-dir ./build",
    "build:prodTest": "bash ./generate-build-info.sh && NODE_ENV=production npm run build:server && NODE_ENV=production UPLOAD_ASSETS=false npm run build:client && npm run tool:checkAssetSize",
    "clean": "rimraf reports",
    "create-html-report": "node node_modules/common-nightwatch/dist/nw-cucumber-conf/create-html-report.js",
    "da-reporter": "node node_modules/.bin/da-reporter -p opxhub-ui -i /reports -c",
    "lint": "eslint --fix src",
    "prod": "CDN_URL=\"//a.travel-assets.com\" NODE_ENV=production node . | rotate-logs --config=.log-access.js | rotate-logs --config=.log-application.js",
    "start": "concurrently --names 'HAPIJS,WBPACK' 'NODE_PORT=8081 npm run start:server' 'npm run start:webpack'",
    "start:prodTest": "DEBUG=catalyst*,serviceclient* BABEL_ENV=server NODE_ENV=production npm run build:prodTest && NODE_ENV=production node-dev --inspect --no-deps --respawn ./build/server/index.js | pino-pretty -ct",
    "start:server": "DEBUG=catalyst*,serviceclient* BABEL_ENV=server NODE_ENV=development node-dev --inspect --no-deps --respawn ./index.js | pino-pretty -ct",
    "start:webpack": "BABEL_ENV=client-dev webpack-dev-server --config ./tools/webpack/development.js",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "BABEL_ENV=test TZ=UTC jest --colors --verbose -u --testNamePattern",
    "test:style": "stylelint --fix src/**/*.less",
    "tool:checkAssetSize": "node ./tools/sizes/checkAssetSize.js",
    "test:automation": "BABEL_ENV=browsertest cucumber-js -- __tests__/automation/features"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/**/*.test.js"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/tools/test-setup.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tools/test-setup.js"
    ],
    "collectCoverage": true,
    "coverageDirectory": "docs/reports/coverage",
    "coverageReporters": [
      "text",
      "json",
      "clover",
      "html"
    ],
    "collectCoverageFrom": [
      "src/universal/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 42,
        "functions": 40,
        "lines": 48,
        "statements": 48
      }
    }
  },
