<!doctype html>
<html lang='en-us'>

<head>
  <title>{{siteName}}</title>
  <meta charset="utf-8" />
  <meta name="description" content="{{description}}">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  {{#if faviconWebAppTags}}
    {{{faviconWebAppTags}}}
  {{else}}
      <link rel="shortcut icon" href="{{ favicon }}" type="image/x-icon">
  {{/if}}
  <link rel="stylesheet" href="{{resolveAsset 'bundle.css'}}">
</head>

<body>

  <div class="container main-app-container">
    {{!-- Main entry point for react app --}}
    <div id="root">{{{body}}}</div>

  </div>
  <script src="{{resolveAsset 'runtime.js'}} "></script>
  <script src="{{resolveAsset 'vendor.js'}} "></script>
  <script src="{{resolveAsset 'bundle.js'}}"></script>


</body>

</html>